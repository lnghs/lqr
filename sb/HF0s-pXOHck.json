[{"text": "- Now here's what you do.", "start": 2.8, "duration": 0.833}, {"text": "You just sneak up in\nfront of 'em and then honk", "start": 3.633, "duration": 3.197}, {"text": "and they do the rest.", "start": 6.83, "duration": 1.54}, {"text": "Watch this.", "start": 8.37, "duration": 0.883}, {"text": "(honking)", "start": 16.439, "duration": 2.167}, {"text": "(mooing)", "start": 19.576, "duration": 2.083}, {"text": "(farting)", "start": 25.172, "duration": 2.109}, {"text": "(laughing)", "start": 27.281, "duration": 2.25}, {"text": "- I swear tractors is so dumb.", "start": 30.444, "duration": 2.5}, {"text": "I'll tell you what buddy.", "start": 34.109, "duration": 1.361}, {"text": "It don't get much better than this.", "start": 35.47, "duration": 1.95}, {"text": "- Yep, you're living the dream Mater boy.", "start": 37.42, "duration": 2.243}, {"text": "(honking)", "start": 43.912, "duration": 2.167}, {"text": "(mooing)", "start": 47.076, "duration": 2.083}, {"text": "(farting)", "start": 51.309, "duration": 1.248}, {"text": "(laughing)", "start": 52.557, "duration": 1.072}, {"text": "I don't care who you are.", "start": 53.629, "duration": 1.099}, {"text": "That's funny right there.", "start": 54.728, "duration": 2.262}, {"text": "Your turn bud.", "start": 56.99, "duration": 1.15}, {"text": "- Mater I can't.", "start": 58.14, "duration": 1.07}, {"text": "I don't even have a horn.", "start": 59.21, "duration": 1.46}, {"text": "- Baby.", "start": 60.67, "duration": 1.37}, {"text": "- I'm not a baby.", "start": 62.04, "duration": 0.833}, {"text": "(clucking)", "start": 62.873, "duration": 1.507}, {"text": "Fine stop, stop okay, all\nright, I'll do something.", "start": 64.38, "duration": 3.257}, {"text": "(revving)", "start": 84.865, "duration": 1.332}, {"text": "(mooing)", "start": 86.197, "duration": 2.083}, {"text": "(farting)", "start": 103.505, "duration": 2.167}, {"text": "(laughing)", "start": 108.274, "duration": 2.25}, {"text": "(mooing)", "start": 113.933, "duration": 2.083}, {"text": "- That's Frank.", "start": 118.685, "duration": 0.949}, {"text": "(upbeat music)", "start": 119.634, "duration": 2.583}]