[{"text": "(upbeat music)", "start": 0.0, "duration": 2.583}, {"text": "- Hang on.", "start": 5.243, "duration": 1.231}, {"text": "- Hanging on.", "start": 6.474, "duration": 1.026}, {"text": "- Hanging on.", "start": 7.5, "duration": 2.128}, {"text": "- Whoa.", "start": 9.628, "duration": 0.833}, {"text": "- Ouch.", "start": 16.679, "duration": 0.833}, {"text": "(upbeat music)", "start": 17.512, "duration": 2.583}, {"text": "- Diminutive ain't they?", "start": 25.574, "duration": 2.066}, {"text": "- We seek the one they call Bugs Bunny.", "start": 27.64, "duration": 2.18}, {"text": "- Yeah, Bugs Bunny.", "start": 29.82, "duration": 0.833}, {"text": "- Have you seen him?", "start": 30.653, "duration": 1.023}, {"text": "- Nope, never heard of him.", "start": 31.676, "duration": 2.99}, {"text": "(fire blast)", "start": 34.666, "duration": 1.674}, {"text": "- Hold on there Mr. Looney Tune.", "start": 36.34, "duration": 2.063}, {"text": "- Okay bunny, gather up your tune pals.", "start": 39.785, "duration": 4.275}, {"text": "- These little guys would\nlike to make an announcement.", "start": 44.06, "duration": 2.5}, {"text": "- You, all of you, are now our prisoners.", "start": 46.56, "duration": 4.395}, {"text": "(group gasps)", "start": 50.955, "duration": 1.891}, {"text": "- Eh, not so fast Doc.", "start": 52.846, "duration": 2.454}, {"text": "You've gotta give us a\nchance to defend ourselves.", "start": 55.3, "duration": 2.783}, {"text": "We challenge you to a basketball game.", "start": 58.083, "duration": 3.847}, {"text": "- All right, basketball it is.", "start": 61.93, "duration": 2.023}, {"text": "- Basketball.", "start": 63.953, "duration": 0.929}, {"text": "- Oh boy, oh boy.", "start": 64.882, "duration": 1.73}, {"text": "- All right.", "start": 66.612, "duration": 1.578}, {"text": "What is basketball?", "start": 68.19, "duration": 2.407}, {"text": "- So sorry.", "start": 70.597, "duration": 1.448}, {"text": "Excuse me.", "start": 72.045, "duration": 0.833}, {"text": "Ouch, get your feet out of my nose.", "start": 74.272, "duration": 1.984}, {"text": "- Quiet they're looking.", "start": 76.256, "duration": 1.998}, {"text": "Hey, it's basketball.", "start": 78.254, "duration": 1.75}, {"text": "- Where?", "start": 81.192, "duration": 1.307}, {"text": "- Whoa, now what?", "start": 82.499, "duration": 2.102}, {"text": "- Let me see.", "start": 84.601, "duration": 1.495}, {"text": "- Look, watch it.", "start": 86.096, "duration": 1.711}, {"text": "(upbeat music)", "start": 87.807, "duration": 2.583}, {"text": "- What are you doing?", "start": 92.815, "duration": 1.75}, {"text": "Timeout, call a timeout.", "start": 96.89, "duration": 1.965}, {"text": "- Let him in, let him in.", "start": 98.855, "duration": 1.413}, {"text": "Open up, open.", "start": 100.268, "duration": 2.362}, {"text": "Wow, you did it.", "start": 102.63, "duration": 1.673}, {"text": "- I got it, I got his talent.", "start": 105.58, "duration": 2.823}, {"text": "- The aliens, it's their\nturn to use the court.", "start": 111.12, "duration": 4.79}, {"text": "- Yeah sure, let the little\npipsqueaks knock themselves out.", "start": 115.91, "duration": 3.58}, {"text": "- Too bad you can't practice\ngetting taller boys.", "start": 119.49, "duration": 2.815}, {"text": "(group laughs)", "start": 122.305, "duration": 2.583}, {"text": "- Whoa.", "start": 126.752, "duration": 0.833}, {"text": "- Whoa.", "start": 130.327, "duration": 0.921}, {"text": "(dramatic music)", "start": 131.248, "duration": 2.75}, {"text": "(monster roar)", "start": 139.823, "duration": 3.202}, {"text": "- Time to play a little basketball.", "start": 143.025, "duration": 2.873}, {"text": "(dramatic music)", "start": 145.898, "duration": 2.75}, {"text": "(upbeat music)", "start": 150.877, "duration": 2.583}, {"text": "- [Narrator] The challengers\nfor the ultimate game,", "start": 159.603, "duration": 3.803}, {"text": "the Monstars.", "start": 163.406, "duration": 1.083}, {"text": "- Go Monstars.", "start": 170.426, "duration": 0.833}, {"text": "Go Monstars.", "start": 171.259, "duration": 0.833}, {"text": "Go Monstars.", "start": 172.092, "duration": 1.102}, {"text": "(upbeat music)", "start": 173.194, "duration": 2.243}, {"text": "- I got it, I got it, I got it,", "start": 175.437, "duration": 0.989}, {"text": "I got the ball, I got the ball.", "start": 176.426, "duration": 1.377}, {"text": "Yikes.", "start": 177.803, "duration": 0.833}, {"text": "- Go away little boy.", "start": 178.636, "duration": 1.75}, {"text": "(upbeat music)", "start": 181.654, "duration": 2.583}, {"text": "- Ooh, this will be good.", "start": 185.21, "duration": 1.303}, {"text": "(upbeat music)", "start": 187.754, "duration": 2.583}, {"text": "(monstar groans)", "start": 195.236, "duration": 2.75}, {"text": "(upbeat music)", "start": 199.445, "duration": 2.583}, {"text": "(group cheers)", "start": 204.599, "duration": 2.583}, {"text": "(upbeat music)", "start": 208.919, "duration": 2.583}, {"text": "- That was so much fun.", "start": 215.826, "duration": 2.004}, {"text": "- I feel so insignificant.", "start": 217.83, "duration": 2.54}, {"text": "- My clothes don't fit.", "start": 220.37, "duration": 1.91}, {"text": "Can we ask you a favor, Mr. Bunny?", "start": 222.28, "duration": 2.39}, {"text": "- Um, I was thinking, can\nwe stay here with you?", "start": 224.67, "duration": 3.353}, {"text": "- Please?", "start": 228.023, "duration": 1.065}, {"text": "- I don't know if you\nguys are loony enough.", "start": 229.088, "duration": 2.171}, {"text": "- Loony enough?", "start": 231.259, "duration": 1.615}, {"text": "(upbeat music)", "start": 232.874, "duration": 2.583}]